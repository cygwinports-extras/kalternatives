  /usr/share/kde4/apps/kalternatives/icons/hicolor/48x48/actions/alternative-broken.png
  /usr/share/kde4/services/kalternatives.desktop
  /usr/share/locale/en_GB/LC_MESSAGES/kalternatives.mo
